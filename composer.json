{
    "name": "kirschbaum-development/laravel-route-file-macro",
    "description": "Route macro that easily loads a route file without unnecessary method calls.",
    "license": "MIT",
    "keywords": [
        "kirschbaum-development",
        "laravel",
        "laravel-route",
        "laravel-route-file-macro"
    ],
    "homepage": "https://github.com/kirschbaum-development/laravel-route-file-macro",
    "support": {
        "issues": "https://github.com/kirschbaum-development/laravel-route-file-macro/issues",
        "source": "https://github.com/kirschbaum-development/laravel-route-file-macro"
    },
    "authors": [
        {
            "name": "David VanScott",
            "email": "david@kirschbaumdevelopment.com",
            "role": "Developer"
        },
        {
            "name": "Justin Seliga",
            "email": "justin@kirschbaumdevelopment.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/routing": ">=5.7",
        "illuminate/support": ">=5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.1"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\RouteFileMacro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirschbaumDevelopment\\RouteFileMacro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\RouteFileMacro\\RouteFileMacroServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}